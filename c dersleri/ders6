#include <stdio.h>

// Parametre alan ve değer döndüren fonksiyon
int addNumbers(int a, int b) {
    return a + b;
}

// Parametre almayan ve değer döndürmeyen fonksiyon
void sayHello() {
    printf("Merhaba!\n");
}

int main() {
    int result = addNumbers(5, 10);
    printf("Toplam: %d\n", result);

    sayHello();

    return 0;
}
