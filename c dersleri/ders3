#include <stdio.h>

int main() {
    int a = 10, b = 20;

    // Aritmetik Operatörler
    printf("Toplama: %d\n", a + b);
    printf("Çıkarma: %d\n", a - b);
    printf("Çarpma: %d\n", a * b);
    printf("Bölme: %d\n", b / a);
    printf("Kalan: %d\n", b % a);

    // Karşılaştırma Operatörleri
    printf("a == b: %d\n", a == b);
    printf("a != b: %d\n", a != b);
    printf("a > b: %d\n", a > b);
    printf("a < b: %d\n", a < b);

    // Mantıksal Operatörler
    int x = 1, y = 0;
    printf("x && y: %d\n", x && y);
    printf("x || y: %d\n", x || y);
    printf("!x: %d\n", !x);

    return 0;
}
